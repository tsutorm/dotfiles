[color]
  # color
  status = auto
  diff = auto
  branch = auto
  interactive = auto
  grep = auto
[alias]
  co = checkout
  st = status
  sh = show
  br = branch
  wc = whatchanged         
  ls = log --stat          
  loga = log -20 --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
  logall = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
[core]
  whitespace = cr-at-eol
  excludesfile = ~/.gitignore

